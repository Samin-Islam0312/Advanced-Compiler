# ARGS: ae
@main {
.entry:
  a: int = const 1;
  b: int = const 2;
  c: bool = gt a b;
  br c .then .else;
.then:
  t1: int = add a b;     # computes expr
  jmp .join;
.else:
  t2: int = add b a;     # same canonical expr
  jmp .join;
.join:
  u: int = add a b;      # available at join entry (on all paths)
  print u;
}

