# ARGS: ae
@main {
.entry:
  a: int = const 1;
  b: int = const 2;
  c: bool = gt a b;
  br c .then .else;
.then:
  t: int = add a b;          # expr computed on THEN only
  jmp .join;
.else:
  a: int = const 3;          # kills availability on ELSE path
  jmp .join;
.join:
  u: int = add a b;          # not available at entry to join (not on all paths)
  print u;
}

